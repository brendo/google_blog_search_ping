<?xml version="1.0" encoding="UTF-8"?>
<extension id="google_blog_search_ping" status="experimental" xmlns="http://symphony-cms.com/schemas/extension/1.0">
	<name>Google Blog Search Ping API</name>
	<description>This extension will track when new entries are created in a section and then ping Google with an RSS/Atom or RDF URL.</description>
	<repo type="github">https://github.com/brendo/google_blog_search_ping</repo>
	<types>
		<type>Workflow</type>
	</types>
	<authors>
		<author>
			<name github="brendo" symphony="brendo">Brendan Abbott</name>
		</author>
	</authors>
	<releases>
		<release version="0.9" date="2011-11-07" min="2.0.6" max="2.2.x" />
	</releases>
</extension>